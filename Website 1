    <script>
        $(document).ready(function(){
        	$("#btn_test1").click(function(){
        	var name_client = $("#Nom").val();
        	var phone_client = $("#Tele").val();
        	var city_client = $("#Ville").val();
        	if(name_client != "" && phone_client != "" && city_client != ""){
        	$("#btn_test1").html('<img style="/*max-height:16px;*/" src="landing/loading.gif" /> المرجو الإنتظار');
			}
		});
        
        });
    </script>
    <script>
        $(document).ready(function(){
        	$("#btn_test2").click(function(){
